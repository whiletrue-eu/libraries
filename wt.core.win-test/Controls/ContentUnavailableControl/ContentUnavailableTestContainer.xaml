<UserControl x:Class="WhileTrue.Controls.ContentUnavailableTestContainer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:dialogPanel="clr-namespace:WhileTrue.Controls"
             xmlns:controls="clr-namespace:WhileTrue.Controls;assembly=wt.core.win"
             mc:Ignorable="d"
             d:DesignHeight="150" d:DesignWidth="400" Padding="10"
             d:DataContext="{d:DesignInstance Type={x:Type dialogPanel:DialogPanelTestContainerValues},IsDesignTimeCreatable=True}">
    <StackPanel>
        <controls:ContentUnavailableControl ContentAvailable="false">
            <!--Controls:ContentUnavailableControl.DefaultContent>
                <TextBlock>Unavailable</TextBlock>
            </Controls:ContentUnavailableControl.DefaultContent-->
            <Label>Available</Label>
        </controls:ContentUnavailableControl>
    </StackPanel>
</UserControl>